{
  "name": "@template-project/template-api",
  "private": true,
  "version": "0.1.0",
  "main": "./src/app.ts",
  "type": "module",
  "scripts": {
    "start": "node build/app.js",
    "build": "tsc",
    "devx": "pnpm with-env nodemon --exec node --loader ts-node/esm ./src/app.ts",
    "dev": "pnpm with-env node --no-warnings --enable-source-maps --loader ts-node/esm .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf node_modules",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../.env --"
  },
  "devDependencies": {
    "@t3-oss/env-nextjs": "^0.7.1",
    "@template-project/template-prisma": "workspace:*",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.42.0",
    "ts-node-dev": "^2.0.0",
    "express": "^4.18.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.6",
    "@clerk/nextjs": "^4.29.3",
    "@template-project/template-prisma": "workspace:*",
    "@trpc/server": "^10.45.0",
    "cors": "^2.8.5",
    "superjson": "^2.2.1",
    "zod": "^3.21.4"
  }
}
